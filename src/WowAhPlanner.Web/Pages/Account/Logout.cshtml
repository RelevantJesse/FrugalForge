@page
@model WowAhPlanner.Web.Pages.Account.LogoutModel
@{
    ViewData["Title"] = "Log out";
}

<h1>Log out</h1>

<form method="post">
    @Html.AntiForgeryToken()
    <div class="card">
        <p>Log out of WowAhPlanner?</p>
        <button type="submit">Log out</button>
        <a href="/" style="margin-left:0.75rem">Cancel</a>
    </div>
</form>

